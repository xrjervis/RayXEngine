<Material name="default">
    <int name="useDiffuse" value="1"/>
    <Texture name="diffuse" value="terrain_32x32" sampler="nearest"/>
    <ShaderProgram name="vertexShader" value="smc_vs"/>
    <ShaderProgram name="pixelShader" value="smc_ps"/>
    <RasterizerState fill="solid" cull="back" frontCounterClockwise="true"/>
    <BlendState blendSrc="src_alpha" blendDst="inv_src_alpha" blendOp="add"/>
    <DepthStencilState depthTestEnable="true" depthTestComapre="less"/>
</Material>